// Place your key bindings in this file to override the defaults
[
  {
    "key": "alt+f2",
    "command": "workbench.action.files.openFolder",
    "when": "openFolderWorkspaceSupport"
  },
  {
    "key": "ctrl+k ctrl+o",
    "command": "-workbench.action.files.openFolder",
    "when": "openFolderWorkspaceSupport"
  },
  {
    "key": "f12",
    "command": "code-runner.run"
  },
  {
    "key": "ctrl+alt+n",
    "command": "-code-runner.run"
  },
  {
    "key": "f12",
    "command": "mysql.runQuery",
    "when": "editorLangId == 'sql'"
  },
  {
    "key": "ctrl+alt+e",
    "command": "-mysql.runQuery",
    "when": "editorLangId == 'sql'"
  },
  {
    "key": "f12",
    "command": "mysql.runSQL",
    "when": "editorLangId == 'cql' || editorLangId == 'dbclient-js' || editorLangId == 'postgres' || editorLangId == 'sql'"
  },
  {
    "key": "ctrl+enter",
    "command": "-mysql.runSQL",
    "when": "editorLangId == 'cql' || editorLangId == 'dbclient-js' || editorLangId == 'postgres' || editorLangId == 'sql'"
  },
  {
    "key": "alt+`",
    "command": "openInIntegratedTerminal"
  },
  {
    "key": "alt+`",
    "command": "-workbench.action.editorDictation.start",
    "when": "hasSpeechProvider && !editorReadonly && !speechToTextInProgress"
  },
  {
    "key": "ctrl+f9",
    "command": "workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "ctrl+shift+b",
    "command": "-workbench.action.tasks.build",
    "when": "taskCommandsRegistered"
  },
  {
    "key": "f11 f12",
    "command": "r.runAllChunks"
  },
  {
    "key": "ctrl+f12",
    "command": "r.knitRmdToHtml"
  },
  {
    "key": "ctrl+1",
    "command": "r.createRTerm"
  },
  {
    "key": "ctrl+enter",
    "command": "r.runCurrentChunk",
    "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && editorLangId == 'rmd'"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "-r.runCurrentChunk",
    "when": "editorTextFocus && editorLangId == 'r' || editorTextFocus && editorLangId == 'rmd'"
  },
  {
    "key": "ctrl+f5",
    "command": "C_Cpp.BuildAndDebugFile"
  },
  {
    "key": "f5",
    "command": "C_Cpp.BuildAndRunFile"
  }
]